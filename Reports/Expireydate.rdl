        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>8b8fcfc2-11b7-4229-b4bc-734289a764f9</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>expirey</DataSourceName>
        <CommandText>SELECT        products.product_ID, products.product_name, products.expire_period, WareHouse.WareHouse_name, product_WareHouse.production_date
FROM            products INNER JOIN
                         product_WareHouse ON products.product_ID = product_WareHouse.product_ID INNER JOIN
                         WareHouse ON product_WareHouse.WareHouse_name = WareHouse.WareHouse_name</CommandText>
      </Query>
      <Fields>
        <Field Name="product_ID">
          <DataField>product_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="product_name">
          <DataField>product_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="expire_period">
          <DataField>expire_period</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="WareHouse_name">
          <DataField>WareHouse_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="production_date">
          <DataField>production_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Expiration_date">
          <Value>=DateAdd("d",Fields!expire_period.Value,Fields!production_date.Value)</Value>
        </Field>
        <Field Name="Days_Till_Expired">
          <Value>=DateDiff("d",Fields!Expiration_date.Value,Now())</Value>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
