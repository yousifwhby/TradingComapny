  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        products.product_name AS ProductName, supplier_requests.date AS InputDate, supplierRequest_details.*
FROM            products INNER JOIN
                         supplierRequest_details ON products.product_ID = supplierRequest_details.product_ID INNER JOIN
                         supplier_requests ON supplierRequest_details.supplierRequest_ID = supplier_requests.inRequest_ID</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InputDate">
          <DataField>InputDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="supplierRequest_ID">
          <DataField>supplierRequest_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="product_ID">
          <DataField>product_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="store_name">
          <DataField>store_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="input_quantity">
          <DataField>input_quantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Production_date">
          <DataField>Production_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PeriodInStrore">
          <Value>=DateDiff("d",Fields!InputDate.Value,now())</Value>
        </Field>
      </Fields>
    </DataSet>
